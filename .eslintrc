{
  "extends": [
    "taro",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "Taro"
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "import/no-commonjs": 0,
    "import/prefer-default-export": 0
  },
  "parser": "babel-eslint"
}
